{
  "name": "code-quarkus-library",
  "version": "0.1.0",
  "scripts": {
    "build": "~/bin/bit link && ~/bin/bit compile",
    "test": "~/bin/bit link && ~/bin/bit test",
    "test:watch": "~/bin/bit link && ~/bin/bit test -w",
    "tag:pr": "~/bin/bit tag --all --soft",
    "tag:release": "~/bin/bit tag --all --persist",
    "watch": "~/bin/bit watch",
    "ci-config": "~/bin/bit config set analytics_reporting false && ~/bin/bit config set anonymous_reporting false",
    "postinstall": "bvm install"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@teambit/bvm": "0.0.26",
    "@testing-library/dom": "8.2.0",
    "@testing-library/react": "12.0.0",
    "@sentry/browser": "6.11.0",
    "classnames": "2.3.1",
    "copy-to-clipboard": "3.3.1",
    "lodash": "4.17.21",
    "react-bootstrap": "1.6.1",
    "react-hotkeys-hook": "3.4.0",
    "react-icons": "4.2.0",
    "use-persisted-state": "0.3.3"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
