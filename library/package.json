{
  "name": "code-quarkus-library",
  "version": "0.1.0",
  "scripts": {
    "build": "~/bin/bit compile",
    "test": "~/bin/bit build",
    "test:watch": "~/bin/bit test -w",
    "tag": " ~/bin/bit tag --all --soft",
    "publish": "~/bin/bit tag --persist && ~/bin/bit export",
    "link": "npm link ./node_modules/@quarkusio/code-quarkus.components || true && npm link ./node_modules/react || true && npm link ./node_modules/react-dom || true",
    "watch": "~/bin/bit watch",
    "ci-config": "~/bin/bit config set analytics_reporting false && ~/bin/bit config set anonymous_reporting false && ~/bin/bit config set user.token $BIT_TOKEN",
    "postinstall": "bvm install && ~/bin/bit import && ~/bin/bit link"
  },
  "dependencies": {
    "@sentry/browser": "6.11.0",
    "@teambit/bvm": "0.0.26",
    "@testing-library/dom": "8.2.0",
    "@testing-library/react": "12.0.0",
    "classnames": "2.3.1",
    "copy-to-clipboard": "3.3.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-bootstrap": "1.6.1",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.4.0",
    "react-icons": "4.2.0",
    "use-persisted-state": "0.3.3"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
